<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap 
PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
"http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="Login">
	<select id="login" resultClass="com.app.bean.AcccountBean"
		parameterClass="com.app.bean.AcccountBean">
		SELECT * FROM tai_khoan WHERE
		EMAIL = #email# AND
		MAT_KHAU = #password#
	</select>
</sqlMap>